        <item name="icon_inject">@drawable/ic_inject_white</item>
        <item name="icon_show_adult">@drawable/show_adult_white</item>
        <item name="icon_help">@drawable/ic_help_white</item>
        <item name="icon_info">@drawable/ic_info_white</item>
        <item name="icon_profile">@drawable/ic_profile_white</item>
        <item name="icon_privacy">@drawable/ic_private_white</item>
        <item name="icon_logout">@drawable/ic_logout_white</item>
        <item name="icon_bookmark">@drawable/collections_bookmark_white</item>
        <item name="icon_system_download">@drawable/ic_system_update_white</item>

        <item name="icon_link_generate">@drawable/link_white</item>

        <item name="icon_api_key">@drawable/key_white</item>
         <!-- Change the overflow icon color -->

        <!-- Status bar color. -->
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowLightStatusBar">true</item>
